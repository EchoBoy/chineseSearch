<!-- miniprogram/pages/setting.wxml -->
<view>
  <block wx:for="{{settings}}" wx:key="id">
    <navigator url="/pages/setting/{{item.page}}" class="setting" hover-class="setting-hover">
      <view class="setting-name">{{item.name}}</view>
      <view class="setting-arrow"></view>
    </navigator>
  </block>
</view>