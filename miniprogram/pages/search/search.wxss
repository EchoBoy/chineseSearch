/* pages/search/search.wxss */
.input-container {
    background-color: var(--theme-color);
    padding: 0 22rpx 30rpx;
}
.search-input {
    color: black;
    padding: 6px 0px 6px 30px;
    line-break: 45px;
    border-radius: 16px;
    font-size: 15px;
}
.search-input-search {
    background: url('data:image/svg+xml;base64,') no-repeat, #ffffff;
    background-size: auto 68%;
    background-position: 3px center;
}

.search-input-loading {
    background: url('data:image/svg+xml;base64,') no-repeat, #ffffff;
    background-size: auto 60%;
    background-position: 3px center;
}
.search-input-succeed {
    background: url('data:image/svg+xml;base64,') no-repeat, #fff;
    background-size: auto 60%;
    background-position: 3px center;
}
.search-input-error {
    background: url('data:image/svg+xml;base64,') no-repeat, #fff;
    background-size: auto 60%;
    background-position: 3px center;
}

.history {
    margin-top: 10px;
    background-color: white;
    padding-left: 10px;
}
.history-title {
    font-size: 12px;
    color: gray;
    height: 35px;
    line-height: 35px;
    font-weight: lighter;
}
.history-item {
    background-color: white;
    border-top: 1px solid #ededed;
    height: 40px;
    line-height: 40px;
    display: flex;
    flex-flow: row;
    align-items: center;
}

.icon {
    margin-right: 8px;
    height: 24px;
    width: 24px;
}
